package testThreadSleep;

public class t1 implements Runnable {

	@Override
	public void run() {
		try{
			this.wait(50000);
		}
		catch(InterruptedException e1){
			System.out.println(e1.getMessage()+" "+e1.getCause()+" ");
			e1.printStackTrace();
		}
		catch (Exception e) {
			e.printStackTrace();
		}
		System.out.println("ajuns1");

	}
	
	public void print(){
		System.out.println("ajuns");
	}

}
